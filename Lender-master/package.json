{
  "name": "lendify",
  "version": "1.0.0",
  "description": "Integrated Lending Platform - All Systems",
  "scripts": {
    "start": "node scripts/start-all.js prod",
    "build": "cd frontend && npm run build",
    "server": "node scripts/start-all.js servers",
    "client": "cd frontend && npm start",
    "dev": "node scripts/start-all.js dev",
    "install:all": "node scripts/install-all.js",
    "update:password": "node scripts/update-mongodb-password.js",
    "fix:password": "node scripts/fix-password-encoding.js",
    "test:mongodb": "cd backend/member-a && node test-connection.js",
    "backend:a": "cd backend/member-a && npm start",
    "backend:b": "cd backend/member-b && npm start",
    "backend:c": "cd backend/member-c && npm start",
    "frontend": "cd frontend && npm start",
    "dev:a": "cd backend/member-a && npm run dev",
    "dev:b": "cd backend/member-b && npm run dev",
    "dev:c": "cd backend/member-c && npm run dev"
